<mvc:View 
	controllerName="app.First" 	
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	height="100%">
    
			<App id="app">
				<Page id="page" title="Table">
					<content>	 

							<Table id="exportTable"
								items="{model>/names}" >
								<columns>
									<Column>
										<Text text="id" />
									</Column>
									<Column>
										<Text text="Name" />
									</Column>
									<Column>
										<Text text="Place" />
									</Column>
								</columns>
							
								<items>
									<ColumnListItem>
										<cells>
											<Text text="{model>id}" />
											<Text text="{model>Name}" />
											<Text text="{model>Place}" />
										</cells>
									</ColumnListItem>
								</items>
							</Table>

					</content>		
				</Page>
			</App>
</mvc:View>